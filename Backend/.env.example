# Model API endpoint to forward requests to
MODEL_API=http://localhost:8000/predict

# Port for the proxy server to listen on
PORT=3000

# Maximum file size in bytes (default: 20MB)
MAX_FILE_BYTES=20971520

# Timeout for requests to model API in milliseconds
FORWARD_TIMEOUT_MS=60000

# Number of retries for failed requests
RETRIES=1